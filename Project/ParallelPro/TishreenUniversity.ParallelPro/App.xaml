<Application x:Class="TishreenUniversity.ParallelPro.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:TishreenUniversity.ParallelPro"
             StartupUri="Windows/MainWindow.xaml">
    <Application.Resources>


        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <!--Order is important-->
                <ResourceDictionary Source="/Styles/Fonts.xaml"/>
                <ResourceDictionary Source="/Styles/Colores.xaml"/>
                <ResourceDictionary Source="/Styles/Textblocks.xaml"/>
                <ResourceDictionary Source="/Styles/Textboxes.xaml"/>
                <ResourceDictionary Source="/Styles/Comboboxes.xaml"/>
                <ResourceDictionary Source="/Styles/Buttons.xaml"/>
                <ResourceDictionary Source="/Styles/Scrolls.xaml"/>
                <ResourceDictionary Source="/Styles/Menus.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
